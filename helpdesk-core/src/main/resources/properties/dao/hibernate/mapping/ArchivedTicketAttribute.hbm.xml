<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="org.esupportail.helpdesk.domain.beans">

	<class name="ArchivedTicketAttribute" table="h_arch_tick_attr" lazy="false">
		<cache usage="nonstrict-read-write" />
		<id name="id">
			<generator class="native">
			    <param name="sequence">s_arch_tick_attr</param>
			</generator>
		</id> 
        <many-to-one name="archivedTicket" column="arch_tick_id" index="i_arch_tick_attr_orde" not-null="true" fetch="join"/> <!-- JOIN-->
        <property name="order" column="orde" index="i_tick_orde" not-null="true"/>
        <property name="name" column="name" not-null="true"/>
        <property name="label" column="labe" not-null="true"/>
        <property name="value" column="valu" length="4096"/>
	</class>

</hibernate-mapping>